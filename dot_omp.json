{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "auto_update": true,
  "console_title_template": "{{.UserName}}{{if .Root}} (Elevated){{end}} @ {{.HostName}} : {{.PWD}}{{if eq .Shell 'pwsh'}} >{{end}}{{if eq .Shell 'bash'}}{{if .Root}} #{{end}}{{if not .Root}} ${{end}}{{end}}",
  "final_space": true,
  "shell_integration": true,
  "version": 3,
  "blocks": [
    {
      "alignment": "left",
      "leading_diamond": "\ue0b6",
      "segments": [
        {
          "background": "#1d2230",
          "foreground": "#a0a9cb",
          "invert_powerline": true,
          // "leading_diamond": "\ue0b6",
          "powerline_symbol": "\ue0b2",
          "properties": {
            "time_format": "2006-01-02 \uf017 15:04:05"
          },
          "style": "diamond",
          "type": "time"
        },
        {
          "background": "#d3d7cf",
          "foreground": "#000000",
          // "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "style": "powerline",
          "type": "os"
        },
        {
          "background": "#ffff66",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0ad ",
          "type": "root"
        },
        {
          "background": "#073278",
          "foreground": "#e4e4e4",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "ssh_icon": "\udb80\udf18"
          },
          "style": "powerline",
          "template": " {{ if .SSHSession }}\udb80\udf18 {{ end }}{{ .UserName }} @ {{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#3465a4",
          "foreground": "#e4e4e4",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "...",
            "home_icon": "\udb80\udedc ",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": "\udb81\udf70 {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#4e9a06",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "trailing_diamond": "\ue0b4",
      "type": "prompt"
    },
    {
      "alignment": "left",
      "leading_diamond": "\ue0b6",
      "newline": true,
      "segments": [
        {
          "background": "#ffa400",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": " \udb83\ude0f {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws"
        },
        {
          "background": "#9ec3f0",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \udb82\udc05 {{ .EnvironmentName }} ",
          "type": "az"
        },
        {
          "background": "#47888d",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \udb84\uddf6 {{ .Project }} :: {{ .Account }} ",
          "type": "gcp"
        }
      ],
      "trailing_diamond": "\ue0b4",
      "type": "prompt"
    },
    {
      "alignment": "left",
      "leading_diamond": "\ue0b6",
      "newline": true,
      "segments": [
        {
          "background": "#00ffff",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue77f {{ .Full }} ",
          "type": "dotnet"
        },
        {
          "background": "#00acd7",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \udb81\udfd3 {{ .Full }} ",
          "type": "go"
        },
        {
          "background": "#4063d8",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \udb82\udf37 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        },
        {
          "background": "#0000ff",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \udb82\udcb1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "lua"
        },
        {
          "background": "#689f63",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}\udb80\udf99 {{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#ffde57",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " \udb80\udf20 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#fffff7",
          "foreground": "#0000ff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \udb81\udfd4 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "r"
        },
        {
          "background": "#ae1401",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \udb83\udd2d {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "#99908a",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \udb85\ude17 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "rust"
        }
      ],
      "trailing_diamond": "\ue0b4",
      "type": "prompt"
    },
    {
      "alignment": "left",
      "leading_diamond": "\ue0b6",
      "newline": true,
      "segments": [
        {
          "background": "#000000",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ],
          "foreground": "#00ff00",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#000000{{ end }}"
          ],
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ if eq .Code 0 }}\u2713{{ else }}\u2717 {{ reason .Code }}{{ end }} ",
          "type": "status"
        },
        {
          "background": "#1d2230",
          "foreground": "#a0a9cb",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf252 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#1d2230",
          "foreground": "#e4e4e4",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "mapped_shell_names": {
              "bash": "\udb84\udd83",
              "pwsh": "\uebc7",
              "sh": "\udb84\udd83"
            }
          },
          "style": "powerline",
          "type": "shell"
        },
        {
          "background": "#000000",
          "foreground": "#d3d7cf",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": "\u276f",
          "type": "status"
        }
      ],
      "trailing_diamond": "\ue0b4",
      "type": "prompt"
    }
  ]
}
