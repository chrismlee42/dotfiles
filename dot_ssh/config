# GitHub
Host github.com
    AddKeysToAgent yes
    Compression yes
    ControlPath no
    ControlMaster no
    ControlPersist no
    ForwardAgent yes
    Hostname github.com
    IdentitiesOnly yes
    PreferredAuthentications publickey

# Gitlab
Host gitlab.com
    AddKeysToAgent yes
    Compression yes
    ControlPath no
    ControlMaster no
    ControlPersist no
    ForwardAgent yes
    Hostname gitlab.com
    IdentitiesOnly yes

Host *
    IdentityFile ~/.ssh/identities/chrismlee42_gmail_com.rsa
    PreferredAuthentications publickey

Host * !github.com !gitlab.com
    AddKeysToAgent yes
    Compression yes
    #ControlPath ~/.ssh/controlmasters/%r@%h:%p
    #ControlMaster auto
    #ControlPersist 10m
    ForwardAgent yes
